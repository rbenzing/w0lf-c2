{
  "version": "0.3.1",
  "server": {
    "host": "127.0.0.1",
    "method": "tls"
  },
  "channels": {
    "tcp": {
      "port": 54678
    },
    "http2": {
      "port": 8080,
      "cert": {
        "key": "server-key.pem",
        "cert": "server-cert.pem"
      }
    },
    "tls": {
      "cert": {
        "key": "server-key.pem",
        "cert": "server-cert.pem"
      },
      "port": 443,
      "version": "TLSv1.2",
      "ciphers": "ECDHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-SHA384:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-SHA256"
    },
    "udp": {
      "type": "udp4",
      "port": 41234
    }
  },
  "encryption": {
    "algorithms": [
      "aes-256-gcm",
      "aes-256-cbc"
    ]
  },
  "logging": {
    "enable": true,
    "fileLogging": true,
    "filename": "server",
    "maxlines": 20000
  },
  "data": {
    "chunk_size": 1024,
    "persist_clients": false
  },
  "path": {
    "downloads": "downloads",
    "plugins": "plugins",
    "logs": "logs",
    "config": "config",
    "certificates": "certs"
  },
  "rateLimit": {
    "maxRequests": 100,
    "maxPerMinute": 10
  }
}